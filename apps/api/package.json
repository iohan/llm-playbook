{
  "name": "@app/api",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --splitting --clean",
    "start": "node dist/index.js",
    "generate-tools": "tsx src/cli/generate-tools.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "migrate": "tsx src/db-migrate.ts",
    "seed": "tsx src/db-seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.62.0",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "mysql2": "^3.14.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "tsup": "^8.1.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0"
  }
}
